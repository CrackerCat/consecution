   "source": [
    "from pandashells import Timer\n",
    "max_depth = 10\n",
    "def recurse(val, depth=0):\n",
    "    if depth > max_depth:\n",
    "        return val\n",
    "    else:\n",
    "        return recurse(val, depth + 1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {
    "collapsed": false
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "__time__,3.582032,time for 1000000\n"
     ]
    }
   ],
   "source": [
    "N = 1000000\n",
    "with Timer('time for {}'.format(N)):\n",
    "    for nn in range(N):\n",
    "        recurse(3)"
   ]
